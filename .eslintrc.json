{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "indent":"off",
        "no-tabs": 0,
        "eol-last": 0,
        "no-underscore-dangle": 0,
        "multiline": 0,
        "no-console": "off",
        "no-restricted-globals": "off",
        "import/no-extraneous-dependencies": 0,
        "object-literal-shorthand": 0,
        "consistent-return": 0,
        "no-prototype-builtins": 0,
        "import/prefer-default-export": 0,
        "no-return-assign": 0,
        "no-undef": 0,
        "no-new": 0,
        "class-methods-use-this": 0,
        "no-plusplus": 0,
        "no-await-in-loop": 0,
        "global-require": 0,
        "no-unused-vars": 0
    },
    "plugins": ["jasmine"]
}
